{
  "cli": {
    "version": ">= 7.0.0",
    "requireCommit": false
  },
  "projectId": "YOUR_EAS_PROJECT_ID",
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildType": "simulator"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildType": "simulator"
      }
    },
    "production": {
      "android": {
        "buildType": "aab"
      },
      "ios": {
        "buildType": "ipa"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccount": "./google-play-key.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "YOUR_ASC_APP_ID"
      }
    }
  }
}
